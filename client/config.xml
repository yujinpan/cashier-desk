<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cashier.desk" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CashierDesk</name>
    <description>Cashier Desk for IOS.</description>
    <author email="1192878390@qq.com" href="https://yujinpan.github.io">
        yujinpan
    </author>
    <content src="index-client.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="blob:app://*/*" />
    <allow-navigation href="blob:app://*/*" />

    <preference name="scheme" value="app" />
    <preference name="hostname" value="localhost" />
    <preference name="Orientation" value="landscape" />
    <preference name="deployment-target" value="12.0" />

