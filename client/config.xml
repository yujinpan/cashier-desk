<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cashier.desk" version="1.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CashierDesk</name>
    <description>Cashier Desk for IOS.</description>
    <author email="1192878390@qq.com" href="https://yujinpan.github.io">
        yujinpan
    </author>
    <content src="index-client.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

    <preference name="scheme" value="app" />
    <preference name="hostname" value="localhost" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
    <preference name="deployment-target" value="12.0" />

    <platform name="ios">
        <icon src="icons/20.png" width="20" height="20" />
        <icon src="icons/29.png" width="29" height="29" />
        <icon src="icons/40.png" width="40" height="40" />
        <icon src="icons/50.png" width="50" height="50" />
        <icon src="icons/57.png" width="57" height="57" />
        <icon src="icons/58.png" width="58" height="58" />
        <icon src="icons/60.png" width="60" height="60" />
        <icon src="icons/72.png" width="72" height="72" />
        <icon src="icons/76.png" width="76" height="76" />
        <icon src="icons/80.png" width="80" height="80" />
        <icon src="icons/87.png" width="87" height="87" />
        <icon src="icons/100.png" width="100" height="100" />
        <icon src="icons/114.png" width="114" height="114" />
        <icon src="icons/120.png" width="120" height="120" />
        <icon src="icons/144.png" width="144" height="144" />
        <icon src="icons/152.png" width="152" height="152" />
        <icon src="icons/167.png" width="167" height="167" />
        <icon src="icons/180.png" width="180" height="180" />
        <icon src="icons/1024.png" width="1024" height="1024" />
    </platform>
</widget>
